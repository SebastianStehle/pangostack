{
  "name": "pangostack-worker",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0 --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "generate-vultr": " node ./scripts/cleanup-openapi.js && npx rimraf ./src/lib/vultr/generated && npx @openapitools/openapi-generator-cli generate -g typescript-fetch -i ./src/lib/vultr/openapi-filtered.json -o ./src/lib/vultr/generated --skip-validate-spec --additional-properties=useSingleRequestParameter=false,ensureUniqueParams=false --schema-mappings=float=number && node ./scripts/add-ts-ignore.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@kubernetes/client-node": "^1.3.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@vultr/vultr-node": "^2.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "execa": "^9.6.0",
    "logform": "^2.7.0",
    "nest-winston": "^1.10.2",
    "node-ssh": "^13.2.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
