{
   "name": "pangostack-backend",
   "private": true,
   "version": "0.0.0",
   "license": "MIT",
   "scripts": {
      "dev": "nest start --watch",
      "mkcert:install": "mkcert --install",
      "mkcert:create": "mkcert -cert-file dev/local-dev.pem -key-file dev/local-dev-key.pem localhost",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
      "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0 --fix",
      "typeorm": "ts-node ./node_modules/typeorm/cli",
      "typeorm:generate-migrations": "npm run typeorm migration:generate -- -d ./src/domain/database/data-source.ts",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "temporal-dev": "temporal server start-dev",
      "generate-worker": "npx rimraf ./src/domain/workers/generated && npx cross-env JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" npx @openapitools/openapi-generator-cli generate -g typescript-fetch --additional-properties=useSingleRequestParameter=false,ensureUniqueParams=false -i http://localhost:3100/api-json -o ./src/domain/workers/generated && node ../scripts/add-ts-ignore.js ./src/domain/workers/generated"
   },
   "dependencies": {
      "@date-fns/utc": "^2.1.1",
      "@fiahfy/ico": "^0.0.5",
      "@nestjs/axios": "^4.0.1",
      "@nestjs/cache-manager": "^3.0.1",
      "@nestjs/common": "^11.1.6",
      "@nestjs/config": "^4.0.2",
      "@nestjs/core": "^11.1.6",
      "@nestjs/cqrs": "^11.0.3",
      "@nestjs/passport": "^11.0.5",
      "@nestjs/platform-express": "^11.1.6",
      "@nestjs/serve-static": "^5.0.3",
      "@nestjs/swagger": "^11.2.0",
      "@nestjs/terminus": "^11.0.0",
      "@nestjs/typeorm": "^11.0.0",
      "@notifo/notifo": "^2.0.2",
      "@temporalio/activity": "^1.12.3",
      "@temporalio/client": "^1.12.3",
      "@temporalio/common": "^1.12.3",
      "@temporalio/worker": "^1.12.3",
      "@temporalio/workflow": "^1.12.3",
      "async-mutex": "^0.5.0",
      "axios": "^1.11.0",
      "bcrypt": "^6.0.0",
      "cache-manager": "^7.1.1",
      "chargebee": "^3.11.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.2",
      "cookie-parser": "^1.4.7",
      "cron-parser": "^5.3.0",
      "cron-schedule": "^5.0.4",
      "date-fns": "^4.1.0",
      "date-fns-tz": "^3.2.0",
      "express-session": "^1.18.2",
      "logform": "^2.7.0",
      "nest-winston": "^1.10.2",
      "passport-custom": "^1.1.1",
      "passport-github2": "^0.1.12",
      "passport-google-oauth2": "^0.2.0",
      "passport-microsoft": "^2.1.0",
      "passport-oauth2": "^1.8.0",
      "passport-saml": "^3.2.4",
      "pg": "^8.14.1",
      "postgresql": "^0.0.1",
      "reflect-metadata": "^0.2.2",
      "rxjs": "^7.8.2",
      "sharp": "^0.34.3",
      "typeorm": "0.3.27",
      "winston": "^3.17.0",
      "yaml": "^2.8.1"
   },
   "devDependencies": {
      "@nestjs/cli": "^11.0.10",
      "@nestjs/schematics": "^11.0.7",
      "@nestjs/testing": "^11.1.6",
      "@types/bcrypt": "^6.0.0",
      "@types/cookie-parser": "^1.4.9",
      "@types/esprima": "^4.0.6",
      "@types/express": "^5.0.3",
      "@types/express-session": "^1.18.2",
      "@types/jest": "^30.0.0",
      "@types/multer": "^2.0.0",
      "@types/node": "^24.3.0",
      "@types/passport-github2": "^1.2.9",
      "@types/passport-google-oauth2": "^0.1.10",
      "@types/passport-jwt": "^4.0.1",
      "@types/passport-local": "^1.0.38",
      "@types/passport-microsoft": "^2.1.0",
      "@types/passport-saml": "^1.1.7",
      "@types/static-eval": "^0.2.34",
      "@types/supertest": "^6.0.3",
      "@types/uuid": "^10.0.0",
      "@typescript-eslint/eslint-plugin": "^8.40.0",
      "@typescript-eslint/parser": "^8.40.0",
      "cross-env": "^10.0.0",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-prettier": "^5.1.3",
      "jest": "^30.0.5",
      "prettier": "^3.6.2",
      "rimraf": "^6.0.1",
      "source-map-support": "^0.5.21",
      "supertest": "^7.1.4",
      "ts-jest": "^29.4.1",
      "ts-loader": "^9.5.1",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.9.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
