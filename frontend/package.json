{
  "name": "pangostack-frontend",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "generate-api": "npx rimraf ./src/api/generated && npx cross-env JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" npx @openapitools/openapi-generator-cli generate -g typescript-fetch --additional-properties=useSingleRequestParameter=false,ensureUniqueParams=false -i https://localhost:3000/api-json -o ./src/api/generated && node gen-helper.mjs && node ../scripts/add-ts-ignore.js ./src/api/generated"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.8",
    "@hookform/resolvers": "^3.3.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.25.0",
    "@types/yaireo__tagify": "^4.18.0",
    "@yaireo/tagify": "^4.22.2",
    "ace-builds": "^1.43.2",
    "classnames": "^2.5.1",
    "color": "^4.2.3",
    "colorjs.io": "^0.6.0-alpha.1",
    "date-fns": "^3.6.0",
    "easymde": "^2.20.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-ace": "^14.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.51.0",
    "react-markdown": "^9.0.1",
    "react-pro-sidebar": "^1.1.0",
    "react-resize-detector": "^10.0.1",
    "react-router-dom": "^6.22.3",
    "react-simplemde-editor": "^5.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-textarea-autosize": "^8.5.3",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.26.4",
    "recharts": "^3.1.2",
    "rehype-external-links": "^3.0.0",
    "rxjs": "^7.8.1",
    "yup": "^1.4.0",
    "zustand": "^4.5.2"
